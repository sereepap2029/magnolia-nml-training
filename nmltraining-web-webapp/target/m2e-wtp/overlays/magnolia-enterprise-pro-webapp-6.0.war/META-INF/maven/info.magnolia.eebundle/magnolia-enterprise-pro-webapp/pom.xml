<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd ">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>info.magnolia.eebundle</groupId>
    <artifactId>magnolia-enterprise-bundle-parent</artifactId>
    <version>6.0</version>
    <relativePath>../pom.xml</relativePath>
  </parent>
  <artifactId>magnolia-enterprise-pro-webapp</artifactId>
  <packaging>war</packaging>
  <name>magnolia-enterprise-pro-webapp</name>
  <description>Magnolia Enterprise Edition Pro pre-installed with several modules.</description>
  <dependencies>
    <dependency>
      <groupId>info.magnolia.eebundle</groupId>
      <artifactId>magnolia-enterprise-standard-webapp</artifactId>
      <type>war</type>
      <exclusions>
        <exclusion>
          <groupId>info.magnolia.widgetset</groupId>
          <artifactId>magnolia-vaadin-widgetset</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>info.magnolia.eebundle</groupId>
      <artifactId>magnolia-enterprise-standard-webapp</artifactId>
      <type>pom</type>
      <exclusions>
        <exclusion>
          <groupId>info.magnolia.widgetset</groupId>
          <artifactId>magnolia-vaadin-widgetset</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <!-- Additional EE modules -->
    <dependency>
      <groupId>info.magnolia.eebundle</groupId>
      <artifactId>magnolia-enterprise-pro-product-descriptor</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.multisite</groupId>
      <artifactId>magnolia-module-multisite</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.personalization</groupId>
      <artifactId>magnolia-personalization-components</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.personalization</groupId>
      <artifactId>magnolia-personalization-widget</artifactId>
    </dependency>

    <dependency>
      <groupId>info.magnolia.widgetset</groupId>
      <artifactId>magnolia-vaadin-widgetset-pro</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.advancedcache</groupId>
      <artifactId>magnolia-advanced-cache</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.advancedcache</groupId>
      <artifactId>magnolia-advanced-cache-app</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.advancedcache</groupId>
      <artifactId>magnolia-advanced-cache-dpc</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.advancedcache</groupId>
      <artifactId>magnolia-advanced-cache-personalization</artifactId>
    </dependency>

    <!-- Magnolia content editor modules -->
    <dependency>
      <groupId>info.magnolia.block</groupId>
      <artifactId>magnolia-block-api</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.editor</groupId>
      <artifactId>magnolia-content-editor-widgetset</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.editor</groupId>
      <artifactId>magnolia-content-editor</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.block</groupId>
      <artifactId>magnolia-block-templating</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia</groupId>
      <artifactId>magnolia-link-unfurl</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.editor</groupId>
      <artifactId>stories-app</artifactId>
    </dependency>

    <dependency>
      <groupId>info.magnolia.contenttags</groupId>
      <artifactId>magnolia-content-tags-core</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.contenttags</groupId>
      <artifactId>magnolia-content-tags-ui</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.periscope</groupId>
      <artifactId>magnolia-periscope-result-ranker</artifactId>
    </dependency>

    <!-- Image recognition and transitive deps -->
    <dependency>
      <groupId>info.magnolia.ai.image</groupId>
      <artifactId>magnolia-image-recognition-api</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.ai.image</groupId>
      <artifactId>magnolia-content-tags-integration</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.ai.image</groupId>
      <artifactId>magnolia-image-recognition</artifactId>
    </dependency>
    <dependency>
      <groupId>info.magnolia.ai.image</groupId>
      <artifactId>magnolia-amazon-image-recognition</artifactId>
    </dependency>

  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.gmaven</groupId>
        <artifactId>gmaven-plugin</artifactId>
        <executions>
          <execution>
            <id>check-for-duplicates</id>
            <phase>verify</phase>
            <goals>
              <goal>execute</goal>
            </goals>
            <configuration>
              <providerSelection>1.5</providerSelection>
              <properties>
                <libdir>${project.build.directory}/${project.build.finalName}/WEB-INF/lib</libdir>
              </properties>
              <source>${project.basedir}/test/check_for_duplicates.groovy</source>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>
